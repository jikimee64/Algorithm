[32m+[m[32m    public long[] solution(long x, int n) {[m
[32m+[m[32m        long[] answer = new long[n];[m
[32m+[m[32m        long temp = x;[m
[32m+[m[32m        for(int i = 0; i < n; i++){[m
[32m+[m[32m            answer[i] = x += temp;[m
[32m+[m[32m            x += temp;[m
[32m+[m[32m        }[m
[32m+[m[32m        return answer;[m
[32m+[m[32m    }[m
