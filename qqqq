[32m+[m[32mclass Greedy_Process {[m
[32m+[m[32m    public void play() {[m
[32m+[m[32m        int n = 3; //ì „ì²´ í•™ìƒ ìˆ˜[m
[32m+[m[32m        int[] lost = {3}; //ì²´ìœ¡ë³µì„ ë„ë‚œë‹¹í•œ í•™ìƒë“¤ì˜ ë²ˆí˜¸[m
[32m+[m[32m        int[] reserve = {1}; //ì—¬ë²Œì˜ ì²´ìœ¡ë³µì„ ê°€ì ¸ì˜¨ í•™ìƒë“¤ì˜ ë²ˆí˜¸[m
[32m+[m[32m        int result = solution(n, lost, reserve); //ì²´ìœ¡ ìˆ˜ì—…ì„ ë“¤ì„ ìˆ˜ ìˆëŠ” í•™ìƒì˜ ìµœëŒ“ê°’[m
[32m+[m[32m        System.out.println("result : " + result);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //í¬ê¸°[m
[32m+[m[32m    public int solution(int n, int[] lost, int[] reserve) {[m
[32m+[m[32m        int answer = n - lost.length;[m
[32m+[m
[32m+[m[32m        //ì—¬ë²Œ ì˜·ì„ ê°€ì§€ê³  ìˆëŠ” í•™ìƒì´ ë„ë‚œ ë‹¹í•˜ë©´ ë¹Œë ¤ì¤„ ìˆ˜ ì—†ë„ë¡ ë§Œë“ ë‹¤.[m
[32m+[m[32m        //-1ì€ ì„ì˜ì˜ ìˆ˜[m
[32m+[m[32m        for (int i = 0; i < lost.length; i++) {[m
[32m+[m[32m            for (int j = 0; j < reserve.length; j++) {[m
[32m+[m[32m                if (lost[i]==reserve[j]) {[m
[32m+[m[32m                    answer++;[m
[32m+[m[32m                    lost[i] = -1;[m
[32m+[m[32m                    reserve[j] = -1;[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //ì˜·ì„ ë¹Œë ¤ì£¼ê³  -1ë¡œ ë§Œë“¤ì–´ ë’¤ì˜ í•™ìƒì—ê²Œ ë¹Œë ¤ì£¼ì§€ ì•Šê²Œ í•œë‹¤.[m
[32m+[m[32m        for(int i=0; i<lost.length; i++) {[m
[32m+[m[32m            for(int j=0; j<reserve.length; j++) {[m
[32m+[m[32m                if(lost[i]==reserve[j]+1 || lost[i]==reserve[j]-1) {[m
[32m+[m[32m                    answer++;[m
[32m+[m[32m                    reserve[j] = -1;[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        return answer;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
